   <section>
        <div class="container mt-5">
            <div class="row">
                {{#each products}}
                <div class="col-md-3 p-3">
                         <div class="card" style="width: 16rem; height:23rem">
                        <img src="/product-images/{{this.product._id}}.jpg" class="card-img-top" alt="..." style="height:12rem ;">
                        <div class="card-body">
                            <h5 class="card-title">{{this.product.Name}}</h5>
                            <p class="card-text">Price:{{this.this.product.Price}}</p>
                            <p class="card-text">Quantity:{{this.quantity}}</p>
                             <p class="card-text">Discription:{{this.product.Discription}}</p>
                        </div>
                        </div>
                        </div>
                        {{/each}}
                </div>
            </div>
        </div>
    </section>
  